data = open('Day 6/input.txt','r').read()

# Need to find a way to advance this by one letter every time it runs. And add a way to check if there's a duplicate letter in a chunk.
def fours(x): 
    chunked_list = list()
    for i in range(0, len(x),4):
        chunked_list.append(x[i:i+4])
        break
    print(chunked_list)

